@model IList<Park>
@{
    ViewData["Title"] = "Home Page";
}

    <section class="centeredPanel">
        <table class="table table-striped">
            @foreach (Park park in Model)
            {
                <tr>
                    <td><a asp-controller="Home" asp-action="Detail" asp-route-id="@park.ParkCode"><img src="~/images/parks/@(park.ParkCode).jpg" /></a></td>
                    <td>
                        <p id="IndexParkName" class="ParkName">@park.ParkName</p>
                        <p id="IndexParkDescription" @park.ParkDescription></p>
                    </td>
                </tr>
            }
        </table>
    </section>